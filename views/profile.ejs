<!DOCTYPE html>
<html lang="en">
<head>
    <!--BEGIN META-->
    <meta content="yes" name="apple-mobile-web-app-capable">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <meta content="Sandy Liu" name="author">
    <meta charset="UTF-8">
    <!--END META-->

    <!--BEGIN TITLE-->
    <title>Gameway | Store</title>
    <!--END TITLE-->

    <!--BEGIN CSS FILES-->
    <!--    <link href="css/profile.css" rel="stylesheet" type="text/css">-->
    <link href="css/mainstyle.css" rel="stylesheet" type="text/css">
    <link href='http://fonts.googleapis.com/css?family=Raleway:600,400,300' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300' rel='stylesheet' type='text/css'>
    <!--END CSS FILES-->
</head>
<body>
    <!--BEGIN MAIN NAVIGATION-->
    <nav class="main-nav">
        <a href="room-list"><h1><i class="fas fa-play-circle"></i></h1></a>
        <a href="home"><h1>HOME</h1></a>
        <a class="current" href="profile"><h1>PROFILE</h1></a>
        <a href="rank"><h1>RANK</h1></a>
        <a href="store"><h1>STORE</h1></a>
        <div class="currency">
            <div class="cash">
                <i class="fas fa-money-bill"></i>
                <span>2147483647</span>
            </div>
            <div class="coins">
                <i class="fas fa-coins"></i>
                <span>2147483647</span>
            </div>
        </div>
    </nav>
    <!--END MAIN NAVIGATION-->

    <div class="social"></div>

    <main>
        <header></header>
        <!--BEGIN PROFILE CONTAINER-->
        <div class="profile-container">
            <!--BEGIN LEFT SIDE-->
            <div class="left-side">
                <form class="profile-search-container">
                    <label for="username"></label>
                    <input type="text" id="username" name="username">
                </form>
                <div class="user-detail">
                    <h3 class="user-id"><i class="fas fa-search"></i> Geoffrey</h3>
                    <div class="avatar-container">
                        <img src="img/avatar.png" alt="user-avatar">
                    </div>
                    <div class="level">
                        <h4 class="xp-level">Level: 32</h4>
                        <div class="xp-bar">
                            <div class="xp-progress-bar"></div>
                        </div>
                        <h4 class="xp-points">77 / 100</h4>
                    </div>
                </div>
                <div class="status-message">
                    <h3>STATUS MESSAGE</h3>
                    <a>Here to have a fun time. I love to play games and I attend University at Buffalo</a>
                </div>
                <div class="current-rank">
                    <h3>CURRENT RANK</h3>
                    <img src="img/gold.png" alt="gold">
                </div>
            </div>
            <!--END LEFT SIDE-->

            <!--BEGIN RIGHT SIDE-->
            <div class="right-side">
                <!--BEGIN RIGHT TOP-->
                <div class="right-top">
                    <div class="total-wins">
                        <h3>TOTAL WINS</h3>
                        <h2>132</h2>
                    </div>
                    <div class="games-played">
                        <h3>GAMES PLAYED</h3>
                        <h2>542</h2>
                    </div>
                    <div class="highest-rank">
                        <h3>HIGHEST RANK</h3>
                        <h2>20</h2>
                    </div>
                </div>
                <!--END RIGHT TOP-->

                <!--BEGIN RIGHT MIDDLE-->
                <div class="right-middle">
                    <div class="achievements">
                        <h3>ACHIEVEMENTS</h3>
                        <div class="achievement-images">
                            <img src="img/achievement1.png" alt="achievement1">
                            <img src="img/achievement2.png" alt="achievement2">
                            <img src="img/achievement3.png" alt="achievement3">
                        </div>
                    </div>
                </div>
                <!--END RIGHT MIDDLE-->

                <!--BEGIN RIGHT BOTTOM-->
                <div class="right-bottom">
                    <div class="match-history">
                        <h3>MATCH HISTORY</h3>
                        <div class="match-wrapper">
                            <div class="match">
                                <h4 class="result">WIN</h4>
                                <h4 class="mode">Normal</h4>
                                <h4 class="length">15:34</h4>
                                <h4 class="xp">99xp</h4>
                                <h4 class="coin">3246</h4>
                                <h4 class="date">02/22/2020</h4>
                            </div>
                        </div>
                        <div class="match-wrapper">
                            <div class="match">
                                <h4 class="result">WIN</h4>
                                <h4 class="mode">Normal</h4>
                                <h4 class="length">15:34</h4>
                                <h4 class="xp">99xp</h4>
                                <h4 class="coin">3246</h4>
                                <h4 class="date">02/22/2020</h4>
                            </div>
                        </div>
                        <div class="match-wrapper">
                            <div class="match">
                                <h4 class="result">WIN</h4>
                                <h4 class="mode">Normal</h4>
                                <h4 class="length">15:34</h4>
                                <h4 class="xp">99xp</h4>
                                <h4 class="coin">3246</h4>
                                <h4 class="date">02/22/2020</h4>
                            </div>
                        </div>
                        <div class="match-wrapper">
                            <div class="match">
                                <h4 class="result">WIN</h4>
                                <h4 class="mode">Normal</h4>
                                <h4 class="length">15:34</h4>
                                <h4 class="xp">99xp</h4>
                                <h4 class="coin">3246</h4>
                                <h4 class="date">02/22/2020</h4>
                            </div>
                        </div>
                        <div class="match-wrapper">
                            <div class="match">
                                <h4 class="result">WIN</h4>
                                <h4 class="mode">Normal</h4>
                                <h4 class="length">15:34</h4>
                                <h4 class="xp">99xp</h4>
                                <h4 class="coin">3246</h4>
                                <h4 class="date">02/22/2020</h4>
                            </div>
                        </div>
                    </div>
                </div>
                <!--END RIGHT BOTTOM-->
            </div>
            <!--END RIGHT SIDE-->
        </div>
        <!--END PROFILE CONTAINER-->
    </main>

    <!--BEGIN FOOTER-->
    <div class="footer-container"></div>
    <!--END FOOTER-->
    <!--BEGIN SCRIPTS-->
    <script crossorigin="anonymous" src="https://kit.fontawesome.com/e584d861f4.js"></script>
    <!--END SCRIPTS-->
</body>
</html>